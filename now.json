{
  "regions": ["all"],
  "builds": [
    {"src": "index.php", "use": "now-php"},
    {"src": "public/**", "use": "@now/static"}
  ],
  "routes": [
      {"src": "/", "dest": "/index.php"},
      {"src": "/public/(.*)", "dest": "/public/$1"},
      {"src": "/images/(.*)", "dest": "/public/images/$1"}
  ]
}
